<main>
    <div class="container">
        <div class="search">
            <div class="search-heading">
                <h2>Avialiable Books</h2>
            </div>
            <div class="book-table">
                <table>
                    <tr>
                        <th>Book</th>
                        <th>Auther</th>
                        <th>Price</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                    <% bookDatas.forEach((individualBookData) => { %> 
                        <tr>
                            <td><%- individualBookData.book %></td>
                            <td><%- individualBookData.author %></td>
                            <td><%- individualBookData.price %></td>
                            <td><a href="/edit/<%= individualBookData._id %>"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a></td>
                            <td><a href="/<%= individualBookData._id %>"><i class="fa fa-trash" aria-hidden="true"></i></a></td>
                        </tr>
                    <% }) %>
                </table>
            </div>
        </div>
    </div>
    
</main>